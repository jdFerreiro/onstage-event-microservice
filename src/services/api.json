  "info": {
    "title": "Events Service API",
    "description": "API documentation for Event Service",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "jwt": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "name": "JWT",
        "description": "Enter JWT token",
        "in": "header"
      }
    },
    "schemas": {
      "EventDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "b1a2c3d4-e5f6-7890-abcd-1234567890ab",
            "description": "ID único del evento (UUID)"
          },
          "title": {
            "type": "string",
            "example": "El Rey León",
            "description": "Título del evento (obra, película, etc.)"
          },
          "description": {
            "type": "string",
            "example": "Musical de Broadway",
            "description": "Descripción del evento"
          },
          "genre": {
            "type": "string",
            "example": "Musical",
            "description": "Género del evento"
          },
          "durationMinutes": {
            "type": "number",
            "example": 120,
            "description": "Duración en minutos"
          },
          "imageUrl": {
            "type": "string",
            "example": "https://ejemplo.com/poster.jpg",
            "description": "URL de la imagen del evento"
          },
          "posterImage": {
            "type": "string",
            "example": "iVBORw0KGgoAAAANSUhEUgAA...",
            "description": "Imagen del póster en formato base64"
          },
          "type": {
            "type": "string",
            "example": "Obra",
            "description": "Tipo de evento (Obra, Película, Concierto, etc.)"
          },
          "releaseDate": {
            "format": "date-time",
            "type": "string",
            "example": "2025-10-01",
            "description": "Fecha de estreno"
          },
          "statusId": {
            "type": "number",
            "example": 2,
            "description": "ID del estatus del evento"
          },
          "preSeasonStart": {
            "format": "date-time",
            "type": "string",
            "example": "2025-09-01T00:00:00Z",
            "description": "Inicio de pre-temporada"
          },
          "preSeasonEnd": {
            "format": "date-time",
            "type": "string",
            "example": "2025-09-15T00:00:00Z",
            "description": "Fin de pre-temporada"
          },
          "preSaleStart": {
            "format": "date-time",
            "type": "string",
            "example": "2025-09-16T00:00:00Z",
            "description": "Inicio de pre-venta"
          },
          "preSaleEnd": {
            "format": "date-time",
            "type": "string",
            "example": "2025-09-30T00:00:00Z",
            "description": "Fin de pre-venta"
          }
        },
        "required": ["title"]
      },
      "FunctionDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "f1a2b3c4-d5e6-7890-abcd-1234567890ab",
            "description": "ID único de la función (UUID)"
          },
          "eventId": {
            "type": "string",
            "example": "b1a2c3d4-e5f6-7890-abcd-1234567890ab",
            "description": "ID del evento asociado"
          },
          "venueId": {
            "type": "string",
            "example": "v1a2b3c4-d5e6-7890-abcd-1234567890ab",
            "description": "ID de la sala (venue)"
          },
          "startTime": {
            "format": "date-time",
            "type": "string",
            "example": "2025-10-01T20:00:00Z",
            "description": "Fecha y hora de inicio de la función"
          },
          "endTime": {
            "format": "date-time",
            "type": "string",
            "example": "2025-10-01T22:00:00Z",
            "description": "Fecha y hora de fin de la función"
          },
          "language": {
            "type": "string",
            "example": "Español",
            "description": "Idioma de la función"
          },
          "format": {
            "type": "string",
            "example": "3D",
            "description": "Formato de la función (2D, 3D, subtitulada, etc.)"
          },
          "statusId": {
            "type": "number",
            "example": 2,
            "description": "ID del estatus de la función"
          },
          "preSaleStart": {
            "format": "date-time",
            "type": "string",
            "example": "2025-09-16T00:00:00Z",
            "description": "Inicio de pre-venta para la función"
          },
          "preSaleEnd": {
            "format": "date-time",
            "type": "string",
            "example": "2025-09-30T00:00:00Z",
            "description": "Fin de pre-venta para la función"
          }
        },
        "required": ["eventId", "venueId", "startTime", "endTime"]
      },
      "ReservationDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "r1a2b3c4-d5e6-7890-abcd-1234567890ab",
            "description": "ID único de la reservación (UUID)"
          },
          "functionId": {
            "type": "string",
            "example": "f1a2b3c4-d5e6-7890-abcd-1234567890ab",
            "description": "ID de la función reservada"
          },
          "seatId": {
            "type": "string",
            "example": "s1a2b3c4-d5e6-7890-abcd-1234567890ab",
            "description": "ID de la butaca reservada"
          },
          "venueId": {
            "type": "string",
            "example": "v1a2b3c4-d5e6-7890-abcd-1234567890ab",
            "description": "ID de la sala (venue)"
          },
          "userId": {
            "type": "string",
            "example": "u1a2b3c4-d5e6-7890-abcd-1234567890ab",
            "description": "ID del usuario que reserva"
          },
          "reservedAt": {
            "format": "date-time",
            "type": "string",
            "example": "2025-09-29T12:00:00Z",
            "description": "Fecha y hora de la reservación"
          },
